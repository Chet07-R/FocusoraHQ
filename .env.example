# Copy this file to `.env.local` and fill in your project-specific values.
# Never commit `.env.local`. Only files ending in `.example` should be tracked.
# All variables MUST start with VITE_ to be exposed to the client via Vite.

# --- Firebase (Required) ---
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=

# --- EmailJS (Optional) ---
VITE_EMAILJS_PUBLIC_KEY=
VITE_EMAILJS_SERVICE_ID=
VITE_EMAILJS_TEMPLATE_ID=

# After filling, run: npm run dev
